            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT     F.id AS idFactura, F.PorcentajeDescuento, F.ITBIS, F.fechaRegistro, F.comentario, V.nombres + ' ' + V.apellido1 + ' ' + V.apellido2 AS nombreVendedor, 
                      CP.descripcion AS condicionPago, CL.nombreComercial, CL.razonSocial, CL.RNC_CED, CL.telefono, DF.cantidad AS cantidadFacturada, 
                      A.descripcion AS descripcionArticulo, C.descripcion AS descripcionCategoria, CL.id
FROM         FACTURAS AS F LEFT OUTER JOIN
                      VENDEDORES AS V ON F.idVendedor = V.id LEFT OUTER JOIN
                      CONDICIONESPAGO AS CP ON F.idCondicionPago = CP.id LEFT OUTER JOIN
                      CLIENTES AS CL ON F.idCliente = CL.id LEFT OUTER JOIN
                      DETALLE_FACTURA AS DF ON DF.idFactura = F.id LEFT OUTER JOIN
                      ARTICULOS AS A ON DF.idArticulo = A.id LEFT OUTER JOIN
                      CATEGORIA AS C ON C.id = A.idCategoria
WHERE     (F.id = @idFactura) OR
                      (CL.id = @idCliente)</CommandText>
